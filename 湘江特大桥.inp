fini
/clear
!湘潭湘江特大桥计算模型(检测荷载)
/PREP7  
!* 
ET,1,LINK1  
ET,2,BEAM3  
R,1,1, ,
R,2,0.1,0.001,0.1, , , ,  
MPTEMP,1,0  
MPDATA,EX,1,,2.1e11 
MPDATA,PRXY,1,,0.3 
TYPE,1  
MAT,1
REAL,1
ll=1*75              !梁全长
hh=10                 !桁梁高
ww=7.5                  !节间长
nn1=ll/ww+1           !一侧主桁下弦纵向节点最大编号
!
!以下完成单侧平面桁架单元的建立
n,1,0,0,0              !下弦第一点
n,nn1,ll,0,0            !下弦最后一点
fill,1,nn1
n,nn1+1,ww,hh,0           !上弦第一点
n,2*nn1-2,ll-ww,hh,0           !上弦最后一点
fill,nn1+1,2*nn1-2
n,2*nn1-1,ww/2,hh/2,0           !再分杆第一点
n,3*nn1-3,ll-ww/2,hh/2,0           !再分杆最后一点
fill,2*nn1-1,3*nn1-3
*DO,ii,1,nn1-1,1          !下弦单元
  e,ii,ii+1
*enddo
*do,ii,nn1+1,2*nn1-3,1      !上弦单元
  e,ii,ii+1
*enddo

*do,ii,1,9,2
	e,ii,ii+20
	e,ii+20,ii+11
*enddo
*do,ii,3,11,2
	e,ii,ii+19
	e,ii+19,ii+9
*enddo
*do,ii,2,10,1
	e,ii,ii+10
	
*enddo
*do,ii,2,10,2
	e,ii,ii+19
	e,ii,ii+20
	
*enddo



TYPE,2   
MAT,1
REAL,2   
!建立纵梁模型
js=1
*Do,ii,1,nn1-1,1 
  k,js,(ii-1)*ww,0,0                       
  k,js+1,ii*ww,0,0
  l,js,js+1
  js=js+2
*ENDDO

D_l=0.1               !网格单元长度0.1
lesize,all,D_l     !划分纵梁单元网格
lmesh,all

!节点耦合
!allsel,all
!cpintf,ux,0.001     !公差
!cpintf,uy,0.001
cc1=NODE(0,0,0)
cp,1,uy,cc1,NNEAR(cc1)  !第一点x,y方向同时约束
cp,2,ux,cc1,NNEAR(cc1)
bh=2
*do,ii,1,nn1-1,1
nsel,s,loc,x,ii*ww
nsel,r,loc,y,0
*GET,cc1,node,,num,min
*GET,jj,node,,count
nnn=cc1
*do,jj_,1,jj,1
  *if,abs(nx(nnn)-ii*ww),gt,0.001,then !控制不要把距离远的点选上
    nsel,u,node,,nnn 
  *endif
  nnn=ndnext(nnn)  
*ENDDO
!cpintf,uy,0.0001
*GET,cc1,node,,num,min
bh=bh+1
cp,bh,uy,cc1,NDNEXT(cc1),NDNEXT(NDNEXT(cc1))
allsel,all
*enddo

!NUMMRG,NODE, , , ,LOW     !合并节点  
!NUMCMP,NODE               !压缩节点编号

/solu
D,1,UX                    !约束，可根据实际情况增加约束
D,1,UY 
D,nn1,UY
!施加荷载

jz_x1=0                       !加载位置起点x坐标（必须为节点）
jz_x2=75                      !加载位置终点x坐标
nHzb=(jz_x2-jz_x1)/D_l+1      !  荷载步
!荷载数组
*DIM,hz,ARRAY,28,2,0,q,l,   
*SET,HZ(1,1,1) , 230000 
*SET,HZ(2,1,1) , 230000 
*SET,HZ(3,1,1) , 230000 
*SET,HZ(4,1,1) , 230000 
*SET,HZ(5,1,1) , 230000 
*SET,HZ(6,1,1) , 230000 
*SET,HZ(7,1,1) , 230000 
*SET,HZ(8,1,1) , 230000 
*SET,HZ(9,1,1) , 230000 
*SET,HZ(10,1,1) , 230000
*SET,HZ(11,1,1) , 230000
*SET,HZ(12,1,1) , 230000
*SET,HZ(13,1,1) , 204000
*SET,HZ(14,1,1) , 204000
*SET,HZ(15,1,1) , 204000
*SET,HZ(16,1,1) , 204000
*SET,HZ(17,1,1) , 204000
*SET,HZ(18,1,1) , 204000
*SET,HZ(19,1,1) , 204000
*SET,HZ(20,1,1) , 204000
*SET,HZ(21,1,1) , 204000
*SET,HZ(22,1,1) , 204000
*SET,HZ(23,1,1) , 204000
*SET,HZ(24,1,1) , 204000
*SET,HZ(25,1,1) , 204000
*SET,HZ(26,1,1) , 204000
*SET,HZ(27,1,1) , 204000
*SET,HZ(28,1,1) , 204000
*SET,HZ(2,2,1) , 1.8
*SET,HZ(3,2,1) , 3.6
*SET,HZ(4,2,1) , 12 
*SET,HZ(5,2,1) , 13.8   
*SET,HZ(6,2,1) , 15.6   
*SET,HZ(7,2,1) , 21.1   
*SET,HZ(8,2,1) , 22.9   
*SET,HZ(9,2,1) , 24.7   
*SET,HZ(10,2,1) , 33.1  
*SET,HZ(11,2,1) , 34.9  
*SET,HZ(12,2,1) , 36.7  
*SET,HZ(13,2,1) , 40.946
*SET,HZ(14,2,1) , 42.696
*SET,HZ(15,2,1) , 49.646
*SET,HZ(16,2,1) , 51.396
*SET,HZ(17,2,1) , 54.388
*SET,HZ(18,2,1) , 56.138
*SET,HZ(19,2,1) , 63.088
*SET,HZ(20,2,1) , 64.838
*SET,HZ(21,2,1) , 67.83 
*SET,HZ(22,2,1) , 69.58 
*SET,HZ(23,2,1) , 76.53 
*SET,HZ(24,2,1) , 78.28 
*SET,HZ(25,2,1) , 81.272
*SET,HZ(26,2,1) , 83.022
*SET,HZ(27,2,1) , 89.972
*SET,HZ(28,2,1) , 91.722
!施加荷载
Delt_Time=D_l/(80*5/18)      !通过每单元的时间（V=80km/h）
*do,ii,1,nHzb,1
  TIME,ii*Delt_Time          !结束时间
  KBC,0                       !台阶加载
  NSUBST,5                    !子步数
  fdele,all,all
  First_hz=(ii-1)*D_l         !第一个荷载位置相对于加载起点位置
  *do,jj,1,28,1
    *if,First_hz-HZ(jj,2,1),ge,0,then
      x_l=jz_x1+First_hz-HZ(jj,2,1)
      f,NODE(x_l,0,0),FY,-HZ(jj,1,1)/2
    *endif
  *enddo
  solve  
*enddo
fini



